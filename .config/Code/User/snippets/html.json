	"imagenes": {
		"prefix": "im",
		"body": [
			"<picture>",
				"\t<source srcset=\"$1.avif\" type=\"image/avif\">",
				"\t<source srcset=\"$1.webp\" type=\"image/webp\">",
                "\t<img loading=\"lazy\" width=\"200\" height=\"300\" src=\"$2\" alt=\"$3\">",

			"</picture>"
