	"Title": {
		"prefix": "title",
		"body": "// Manuel Oscar Delpino\n// Ejercicio ${1:number}: ${2:description}\n",
		"description": "Titulo completo de ejercicio"
	},
	
	"Standard Input/Output": {
		"prefix": "stdio",
		"body": "#include <stdio.h>\n",
		"description": "Include Standard Input Output (C Library)"
	},

	"CType": {
		"prefix": "ctype",
		"body": "#include <ctype.h>\n",
		"description": "Include CType (C Library)"
	},
	
	"String": {
		"prefix": "string",
		"body": "#include <string.h>",
		"description": "Include String (C Library)"
	}
	
